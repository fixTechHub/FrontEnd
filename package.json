{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "dev:user": "npm run dev -w user-app",
        "dev:admin": "npm run dev -w admin-app",
        "dev": "concurrently \"npm:dev:*\"",
        "build:user": "npm run build -w user-app",
        "build:admin": "npm run build -w admin-app",
        "build": "npm run build:user && npm run build:admin"
    },
    "devDependencies": {
        "concurrently": "^8.0.0"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/material": "^7.3.1",
        "date-fns": "^4.1.0",
        "react-calendar": "^6.0.0"
    }
}
