{
  "buildCommand": "cd apps/admin-app && npm install && npm run build",
  "outputDirectory": "apps/admin-app/dist",
  "installCommand": "npm install",
  "functions": {
    "apps/admin-app/**": {
      "runtime": "nodejs22.x"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "22"
    }
  },
  "rewrites": [
    {
      "source": "/admin-app/(.*)",
      "destination": "/apps/admin-app/$1"
    }
  ]
} 